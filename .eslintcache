{\n    //randomized background image with reload. Will A\n    let array = [first, second, third, fourth, fifth, sixth, eighth, nineth, tenth]\n    let img = Math.floor( Math.random() * 9 )\n\n    return (\n      \n      <div className=\"App\" style={{backgroundImage: `url(${array[img]})`, \n        height: '100%'}}>\n        <Navbar icon=\"puzzle\" title=\"Empowered People\" description=\"Explore Your Imagination\"/>\n        <Switch>\n          {/* <Route exact path=\"/games\" component={Game}/> */}\n          <Route exact path=\"/games\" component={Editor}/>\n          <Route path=\"/games/play\" component={Engine}/>\n          <Route exact path=\"/empowered_people\" component={Home}/>\n          <Route  exact path=\"/posts\" component={Post}/>\n          <Route exact path='/posts/new' component={PostForm}/>\n          <Route path=\"/users\" component={User} />\n          <Route path=\"/login\" component={Login} />\n          <Route path=\"/learn\" component={ExtinctAnimals} />\n          <Route path=\"/signup\" component={Signup} />\n        </Switch>\n      </div>\n    ); \n  }\n}\n\nconst mapDispatchToProps = {\n  fetchTheDeadWorks,\n  currentUser\n}\n\nconst mapStateToProps = (state) =>{\n  return{\n  users: state.users,\n  auth: state.auth\n}}\n\nApp.propTypes = {\n  message: PropTypes.string.isRequired,\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(withRouter(App));\n","/mnt/c/Users/webb.justin15/Final-Project/empowered_people_app/src/reportWebVitals.js",[],"/mnt/c/Users/webb.justin15/Final-Project/empowered_people_app/src/reducers/index.js",[],"/mnt/c/Users/webb.justin15/Final-Project/empowered_people_app/src/actions/auth.js",[],"/mnt/c/Users/webb.justin15/Final-Project/empowered_people_app/src/components/Navbar.js",[],"/mnt/c/Users/webb.justin15/Final-Project/empowered_people_app/src/actions/thedead.js",[],"/mnt/c/Users/webb.justin15/Final-Project/empowered_people_app/src/components/PostForm.js",["231"],"import React from 'react'\nimpo